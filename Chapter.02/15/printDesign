public static void printDesign() {
    for(int i = 1; i <= 5; i++){
        for(int j = 6 - i; j >= 1; j--){
            System.out.print("-");
        }
        for(int k = 1; k <= 2 * i - 1; k++){
            System.out.print(2 * i - 1);
        }
        for(int j = 6 - i; j >= 1; j--){
            System.out.print("-");
        }
        System.out.println();
    }
}
