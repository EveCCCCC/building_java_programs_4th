public static boolean isAllVowels(String s){
	s = s.toLowerCase();
    int l = s.length();
    boolean isAll = true;
	for(int i = 0; i <= l - 1; i++){
    	if(s.charAt(i) != 'a' && s.charAt(i) != 'e' &&
           s.charAt(i) != 'i' && s.charAt(i) != 'o' &&
           s.charAt(i) != 'u'){
        	isAll = false;
        }
    }
    return isAll;
}
