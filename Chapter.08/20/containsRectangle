public boolean contains(int x, int y){
    if(this.x <= x && x <= this.x + width && this.y <= y && y <= this.y + height){
        return true;
    } else {
        return false;
    }
}
public boolean contains(Point p){
    if(this.x <= p.getX() && p.getX() <= this.x + width && this.y <= p.getY() && p.getY() <= this.y + height){
        return true;
    } else {
        return false;
    }
}
