public class Rectangle{
    int x;
    int y;
    int width;
    int height;
    public Rectangle(int x, int y, int width, int height){
        this.x = x;
        this.y = y;
        this.width = width;
        this.height = height;
    }
    public int getHeight(){
        return this.height;
    }
    public int getWidth(){
        return this.width;
    }
    public int getX(){
        return this.x;
    }
    public int getY(){
        return this.y;
    }
    public String toString(){
        return "Rectangle[x=" + this.x + ",y=" + this.y +
               ",width=" + this.width + ",height=" + this.height + "]";
    }
}
